{{ 'column.css' | asset_url | stylesheet_tag }}


<section>
  <div class="column_section">
   <div class="column_header">
      <h2>{{ section.settings.header }}</h2>
   </div>
   <div class="column_line">
      <p>{{ section.settings.line }}</p>
    </div>
    <div class="star">
       {%- for block in section.blocks -%}
      <div class="column_star">
        <svg height="25" width="23" class="star rating" data-rating="4" stroke="#cd9944">
           <polygon points="9.9, 1.1, 3.3, 21.78, 19.8, 8.58, 0, 8.58, 16.5, 21.78" style="fill-rule:nonzero;"/>
       </svg> 
      </div> 
    {% endfor %}
    </div>
    <div class="head">
     <h3>{{ section.settings.head}}</h3>
    </div>
    <div class="head">
     <p>{{ section.settings.head_text}}</p>
    </div>
    <div class="footer_head">
      <h3>{{ section.settings.footer_header }}</h3>
    </div>
      
  </div>
            

  <div class="section-2">
       <div class="star_2">
       {%- for block in section.blocks -%}
      <div class="column_star-2">
        <svg height="25" width="23" class="star rating" data-rating="4" stroke="#cd9944">
           <polygon points="9.9, 1.1, 3.3, 21.78, 19.8, 8.58, 0, 8.58, 16.5, 21.78" style="fill-rule:nonzero;"/>
       </svg> 
      </div> 
    </div>
    {% endfor %}
    <div class="head-2">
     <h3>{{ section.settings.head}}</h3>
    </div>
    <div class="head-2">
     <p>{{ section.settings.head_text}}</p>
    </div>
    <div class="footer_head-2">
      <h3>{{ section.settings.footer_header }}</h3>
    </div>
  </div>
 
</section>











