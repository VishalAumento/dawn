{{ 'column.css' | asset_url | stylesheet_tag }}


<section>
  <div class="column_section">
   <div class="column_header">
      <h2>{{ section.settings.header }}</h2>
   </div>
   <div class="column_line">
      <p>{{ section.settings.line }}</p>
    </div>
      <div class="column_star">
      {%- for block in section.blocks -%}
         <img src="{{ block.settings.star_images | img_url: 'master'}}">
      {% endfor %}
    </div> 
  </div>
</section>











{% schema %}
 {
  "name": "Column",
  "tag": "section",
  "settings": [
    {
      "type": "text",
      "id": "header",
      "label": "Heading",
      "default": "Quick links"
    },
    {
      "type": "text",
      "id": "line",
      "label": "Heading",
      "default": "Quick links"
    }
  ],
  "blocks": [
  {
  "type": "images",
  "name": "Images",
  "settings": [
     {
      "type": "image_picker",
      "id": "star_images",
      "label": "image"
    }
  ] 
  }
],
  "presets": [
     {
       "name": "Column",
       "settings": {
          
        }
     }
   ]
}
{% endschema %}

