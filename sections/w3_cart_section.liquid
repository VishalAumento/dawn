{{ 'w3_cart_section.css' | asset_url | stylesheet_tag }}

<div class="cart_section">
  {%- for block in section.blocks -%}
  <div class="cart">
    <div class="cart_header">
      <h3>{{ block.settings.cart_head }}</h3>
    </div>
    <div class="cart_text_section">
      <p>{{ block.settings.cart_text }}</p>
    </div>
    <div class="cart_button">
     <a href="#"><p>{{ block.settings.cart_button }}</p></a>
    </div>
  </div>
  {% endfor %}
</div>









{% schema %}
  {
  "name": "w3_cart_section",
  "tag": "section",
  "settings": [
 
  ],
  "blocks": [
    {
      "type": "heading",
      "name": "t:sections.image-banner.blocks.heading.name",
      "settings": [
        {
          "type": "text",
          "id": "cart_head",
          "label": "Heading",
          "default": "Quick links"
        },
        {
          "type": "text",
          "id": "cart_text",
          "label": "text",
          "default": "Quick links"
        }
    ]
  }
 ],
  "presets": [
     {
       "name": "w3_cart_section",
       "settings": {
          
        }
     }
   ]
}
{% endschema %}

