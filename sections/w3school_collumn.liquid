{{ 'w3school_collumn.css' | asset_url | stylesheet_tag }}

<div class="collumn_section">
  {%- for block in section.blocks -%}
  <div class="collumn">
      <h3>{{ block.settings.head }}</h3>
      <p>{{ block.settings.collumn_text }}</p>
  </div>
  {% endfor %}
</div>













{% schema %}
{
"name": "w3school_collumn",
"tag": "section",
"settings": [
  
],
 "blocks": [
    {
      "type": "heading",
      "name": "t:sections.image-banner.blocks.heading.name",
      "settings": [
        {
          "type": "text",
          "id": "head",
          "label": "Heading",
          "default": "Quick links"
        },
        {
          "type": "text",
          "id": "collumn_text",
          "label": "collumn_text",
          "default": "Quick links"
        }
    ]
  }
 ],
"presets": [
   {
     "name": "w3school_collumn",
     "settings": {
        
      }
   }
 ]
}
{% endschema %}

